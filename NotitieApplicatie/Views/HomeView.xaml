<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NotitieApplicatie.Views"
             xmlns:Viewmodels="clr-namespace:NotitieApplicatie.Viewmodels" x:Class="NotitieApplicatie.Views.HomeView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">


    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ContentControl Content="{Binding NotitieBoekLijst}" Grid.Column="0"></ContentControl>
        <ContentControl Content="{Binding NotitieBoek}" Grid.Column="1"></ContentControl>
        <ContentControl Content="{Binding NotitieLijst}" Grid.Column="2"></ContentControl>



    </Grid>






</UserControl>
